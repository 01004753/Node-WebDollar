{
  "presets": ["es2017","env"],
  "plugins": [
    ["module-alias", [
      {"src": "./src/applications",  "expose": "applications"},
      {"src": "./src/blockchain",  "expose": "blockchain"},
      {"src": "./src/node", "expose":"node"},
      {"src": "./src/consts", "expose":"consts"},
      {"src": "./src/node", "expose":"node"},
      {"src": "./src/common", "expose":"common"},
      {"src": "./src", "expose":"src"},
      {"src": "./src/tests", "expose":"tests"}
    ]]
  ]
}
