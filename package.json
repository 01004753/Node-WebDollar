{
  "name": "node-webdollar",
  "version": "0.0.1",
  "description": "Node of WebDollar",
  "main": "index.js",
  "scripts": {
    "build_terminal": "babel --presets es2017 src -d dist && browserify dist/build_terminal.js > dist_bundle/bundle_terminal.js",
    "build_browser": "browserify src/build_browser.js -o dist_bundle/../../vue-Frontend/public/WebDollar-dist/Node-WebDollar-bundle.js -t [ babelify --presets [ es2017 ] ]",
    "test_browser": "browserify src/build_browser.js -o dist_bundle/browser/bundle_browser.js -t [ babelify --presets [ es2017 ] ]",

    "test_double_connections": "npm run build_terminal && ALLOW_DOUBLE_CONNECTIONS=true node dist/build_terminal.js  ",
    "test": "npm run build_terminal && PARAM=6000 node dist/build_terminal.js  "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebDollar/Node-WebDollar.git"
  },
  "keywords": [
    "Node",
    "CryptoCurrency",
    "sockets"
  ],
  "author": "WebDollar - Alexandru Ionut Budisteanu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WebDollar/Node-WebDollar/issues"
  },
  "homepage": "https://github.com/WebDollar/Node-WebDollar#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "babelify": "^7.3.0",
    "browserify": "^14.5.0",
    "browserify-fs": "^1.0.0",
    "bufferutil": "^3.0.2",
    "colors": "^1.1.2",
    "fs": "0.0.1-security",
    "ipaddr.js": "^1.5.4",
    "jsonp": "^0.2.1",
    "node-jsonp": "^1.0.2",
    "rxjs": "^5.4.3",
    "socket.io": "^2.0.3",
    "socket.io-browserify": "^0.9.6",
    "socket.io-client": "^2.0.3",
    "socket.io-p2p": "^2.2.0",
    "utf-8-validate": "^3.0.3",
    "wrtc": "0.0.62"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2017": "^6.24.1",
    "babelify": "^7.3.0"
  }
}
