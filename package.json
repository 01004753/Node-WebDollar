{
  "name": "node-webdollar",
  "version": "0.0.1",
  "description": "Node of WebDollar",
  "main": "index.js",
  "scripts": {
    "build_terminal": "babel --presets es2017 src -d dist && browserify dist/build_terminal.js > dist_bundle/bundle_terminal.js",
    "build_brower_test": "browserify src/build_browser.js -o dist_bundle/browser/bundle_browser.js -t [ babelify --presets [ es2017 ] ]",
    "build_browser": "browserify src/build_browser.js -o dist_bundle/../../vue-Frontend/public/WebDollar-dist/Node-WebDollar-bundle.js -t [ babelify --presets [ es2017 ] ] && npm run build_brower_test",
    "start_double_connections": "npm run build_terminal && ALLOW_DOUBLE_CONNECTIONS=true DEBUG_SIGNALING_SERVER=true  node dist/build_terminal.js  ",
    "start": "npm run build_terminal && TESTING=true node dist/build_terminal.js  ",
    "test": "npm run build_terminal && mocha --timeout 5000 dist/tests/main.test.js --reporter mochawesome && npm run show_mocha_report",
    "show_mocha_report": "node src/tests/show-mocha.test.js",
    "coverage": "npm run build_terminal  && istanbul cover _mocha dist/tests/main.test.js -x *.test.js && npm run show_coverage",
    "show_coverage": "node src/tests/show-coverage.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebDollar/Node-WebDollar.git"
  },
  "keywords": [
    "p2p",
    "Blockchain",
    "Digital Currency",
    "Crypto Currency",
    "Sockets",
    "webrtc"
  ],
  "author": "WebDollar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WebDollar/Node-WebDollar/issues"
  },
  "homepage": "https://github.com/WebDollar/Node-WebDollar#readme",
  "dependencies": {
    "argon2": "^0.16.2",
    "argon2-browser": "^1.1.0",
    "axios": "^0.16.2",
    "babel-plugin-module-alias": "^1.6.0",
    "babelify": "^7.3.0",
    "big-integer": "^1.6.25",
    "bignumber": "^1.1.0",
    "bignumber.js": "^4.1.0",
    "browserify": "^14.5.0",
    "browserify-fs": "^1.0.0",
    "bs58": "^4.0.1",
    "buffer": "^5.0.8",
    "bufferutil": "^3.0.2",
    "colors": "^1.1.2",
    "easyrtc": "^1.1.0",
    "elliptic": "^6.4.0",
    "fs": "0.0.1-security",
    "get-random-values": "^1.2.0",
    "indexeddb-js": "0.0.14",
    "ipaddr.js": "^1.5.4",
    "javascript-biginteger": "^0.9.2",
    "jsonp": "^0.2.1",
    "node-jsonp": "^1.0.2",
    "opn": "^5.1.0",
    "rxjs": "^5.4.3",
    "secp256k1": "^3.3.1",
    "simple-peer": "^8.1.1",
    "socket.io": "^2.0.3",
    "socket.io-browserify": "^0.9.6",
    "socket.io-client": "^2.0.3",
    "socket.io-p2p": "^2.2.0",
    "utf-8-validate": "^3.0.3",
    "wrtc": "0.0.62"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2017": "^6.24.1",
    "babelify": "^7.3.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "mochawesome": "^2.3.1"
  }
}
