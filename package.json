{
  "name": "@webdollar/node-webdollar",
  "version": "0.0.2",
  "description": "Node of WebDollar",
  "main": "index.js",
  "scripts": {
    "build_terminal": "babel --presets es2017 src -d dist && browserify dist/build_terminal.js > dist_bundle/bundle_terminal.js",
    "show_browser_test": "node src/tests/browser/show-browser.test.js",
    "build_browser_test": "browserify src/build_browser.js -o dist_bundle/browser/bundle_browser.js -t [ babelify --presets [ es2017 ] ] && npm run show_browser_test",
    "build_browser": "browserify src/build_browser.js -o dist_bundle/../../vue-Frontend/public/WebDollar-dist/WebDollar-Protocol-bundle.js -t [ babelify --presets [ es2017 ] ] ",
    "build_browser_user_interface": "browserify src/build_browser.js -o dist_bundle/../../User-Interface-WebDollar/dist_bundle/WebDollar-Protocol-bundle.js -t [ babelify --presets [ es2017 ] ] ",
    "start_double_connections": "npm run build_terminal && DEBUG_SIGNALING_SERVER=true  node dist/build_terminal.js  ",
    "start": "npm run build_terminal && node dist/build_terminal.js  ",
    "start_mining": "npm run build_terminal && START_MINING=true SERVER_PORT=6666  node dist/build_terminal.js ",
    "test_node": "npm run build_terminal && START_TESTING=true node dist/build_terminal.js ",
    "test": "npm run build_terminal && mocha --exit --timeout 10000 dist/tests/main.test.js --reporter mochawesome && npm run show_mocha_report",
    "show_mocha_report": "node src/tests/browser/show-mocha.test.js",
    "coverage": "npm run build_terminal  && istanbul cover _mocha dist/tests/main.test.js -x *.test.js && npm run show_coverage",
    "show_coverage": "node src/tests/browser/show-coverage.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebDollar/Node-WebDollar.git"
  },
  "keywords": [
    "p2p",
    "Blockchain",
    "Digital Currency",
    "Crypto Currency",
    "Sockets",
    "webrtc"
  ],
  "author": "WebDollar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WebDollar/Node-WebDollar/issues"
  },
  "homepage": "https://github.com/WebDollar/Node-WebDollar#readme",
  "dependencies": {
    "argon2": "^0.17.0",
    "argon2-browser": "^1.1.0",
    "atob": "^2.0.3",
    "axios": "^0.16.2",
    "babel-plugin-module-alias": "^1.6.0",
    "babelify": "^7.3.0",
    "big-integer": "^1.6.26",
    "bignumber.js": "^4.1.0",
    "bitcoinjs-lib": "^3.3.2",
    "blob-to-buffer": "^1.2.6",
    "browserify": "^14.5.0",
    "browserify-fs": "^1.0.0",
    "bs58": "^4.0.1",
    "btoa": "^1.1.2",
    "buffer": "^5.0.8",
    "bufferutil": "^3.0.3",
    "colors": "^1.1.2",
    "elliptic": "^6.4.0",
    "fs": "0.0.1-security",
    "get-random-values": "^1.2.0",
    "indexeddb-js": "0.0.14",
    "ipaddr.js": "^1.5.4",
    "md5": "^2.2.1",
    "node-gyp": "^3.6.2",
    "node-jsonp": "^1.0.2",
    "opn": "^5.1.0",
    "pouchdb": "^6.4.1",
    "pouchdb-node": "^6.3.4",
    "schnorr": "0.0.2",
    "secp256k1": "^3.3.1",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "utf-8-validate": "^3.0.4",
    "uuid": "^3.1.0",
    "webworkify": "^1.5.0",
    "wrtc": "0.0.62"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2017": "^6.24.1",
    "babelify": "^7.3.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "mochawesome": "^2.3.1"
  }
}
